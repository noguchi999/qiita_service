.container
  .content
    .page-header
      %h1= @profile["name"]

    %table
      %thead
        %tr
          %th
          %th
          %th

      %tbody
        %tr
          %td  Profile
          %td= @profile["name"]
          %td= @profile

        - @friends.each do |friend|
          %tr
            %td  Friends
            %td= friend["name"]
            %td= friend

        - @likes.each do |like|
          %tr
            %td  Likes
            %td= like
            %td= like

        - @feeds.each do |feed|
          %tr
            %td  Feeds
            %td= feed["message"]
            %td= feed

        - @results.each do |result|
          %tr
            %td  Results
            %td= result
            %td= result

    %br
    = link_to 'New Timeline', new_tomodachi_path

  = render 'footer'